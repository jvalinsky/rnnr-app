{
  "expo": {
    "name": "rnnr-rn-app",
    "description": "This project is really great.",
    "slug": "rnnr-rn-app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
       "assets/db/*"
    ],
    "ios": {
      "supportsTablet": true
    },
  "packagerOpts": {
    "assetExts": ["db", "sqlite", "db.sqlite"]
  },
  }
}
